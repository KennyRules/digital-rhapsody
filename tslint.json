{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-eslint-rules"
  ],
  "rules": {
    "max-line-length": [true, 100],
    "no-console": false,
    "no-namespace": [
      true,
      "allow-declarations"
    ],
    "quotemark": [true, "single","jsx-single"]
  }
}